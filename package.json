{
  "name": "ref-portlet",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc",
    "local": "ng serve -c=local --open --port 4207",
    "dev": "ng serve -c=dev --open --port 4207",
    "qa": "ng serve -c=qa --open --port 4207",
    "prod": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve -c=production --open --port 4207",
    "build:local": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build -c=local --configuration production",
    "build:dev": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build -c=dev --configuration production",
    "build:qa": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build -c=qa --configuration production",
    "build:prod": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng build -c=production --configuration production",
    "test": "ng test",
    "lint": "ng lint --fix oa",
    "lint:log": "ng lint --format stylish > lint.log",
    "e2e": "ng e2e",
    "analysis": "ng build && source-map-explorer dist/build/main.bundle.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.4",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/material": "^14.2.4",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@types/crypto-js": "^4.0.2",
    "bootstrap": "^4.6.1",
    "caniuse-lite": "^1.0.30001344",
    "chart.js": "^3.8.0",
    "core-js": "^2.6.5",
    "cropperjs": "^1.5.12",
    "crypto-js": "^4.1.1",
    "d3": "^7.4.4",
    "font-awesome": "^4.7.0",
    "moment": "^2.24.0",
    "ng-recaptcha": "^10.0.0",
    "ng2-charts": "^3.0.11",
    "ng2-file-upload": "^1.4.0",
    "ng2-pdf-viewer": "^9.0.0",
    "ngx-avatars": "^1.3.2",
    "ngx-skeleton-loader": "^6.0.0",
    "ngx-toastr": "^14.3.0",
    "prismjs": "^1.16.0",
